{
  "name": "frontend-ops-torino",
  "version": "0.0.1",
  "description": "Scaffold example",
  "main": "dist/umd-min/index.js",
  "scripts-info": {
    "info": "Displays information about the scripts.",
    "test": "Run test suite once.",
    "lint": "Lint the code",
    "validate": "Run tests and linter for the project.",
    "release:major": "Bump the project of one Major version creating a git tag, updating package.json and updating the changelog file. This command expect git to be in a clean status, it also push changes to the server.",
    "release:minor": "Bump the project of one Minor version creating a git tag, updating package.json and updating the changelog file. This command expect git to be in a clean status, it also push changes to the server.",
    "release:patch": "Bump the project of one Patch version creating a git tag, updating package.json and updating the changelog file. This command expect git to be in a clean status, it also push changes to the server.",
    "docs:clean": "Delete gitbook generate folders.",
    "docs:build": "Build documentation with gitbook.",
    "docs:watch": "Run gitbook documentation in a local live server.",
    "docs:publish": "Build gitbook documentation and publish it to github pages."
  },
  "scripts": {
    "info": "npm-scripts-info",
    "test": "cross-env NODE_ENV=test karma start",
    "lint": "eslint src tests",
    "validate": "npm run test && npm run lint",
    "release:major": "node ./scripts/release.js -a major",
    "release:minor": "node ./scripts/release.js -a minor",
    "release:patch": "node ./scripts/release.js -a patch",
    "docs:clean": "node ./scripts/docs.js -a clean",
    "docs:build": "node ./scripts/docs.js -a build",
    "docs:watch": "node ./scripts/docs.js -a watch",
    "docs:publish": "node ./scripts/docs.js -a publish"
  },
  "contributors": [
    "Matteo Ronchi <mronchi@workwave.com> (http://www.workwave.com/)"
  ],
